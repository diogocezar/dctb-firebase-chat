{
	"database": {
		"rules": "database.rules.json"
	},
	"hosting": {
		"cleanUrls": true,
		"public": "./",
		"rewrites": [
			{
				"source": "/teste",
				"destination": "/teste.html"
			},
			{
				"source": "/produto/",
				"destination": "/produto.html"
			},
			{
				"source": "/",
				"destination": "/index.html"
			}
		],
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/.git/**"
		]
	}
}
